<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <connectionStrings>

  </connectionStrings>
  <appSettings>
    <add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
    <add key="Factory" value="Sql" />
  </appSettings>
  
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="NewBinding0" />
        <binding name="BindingBasicHttpUserCredential">
          <security mode="Transport">
           
          </security>
        </binding>
      </basicHttpBinding>
    </bindings>
    <services>
      <service name="MyAirport.Service.AirportService">
        <endpoint address="" binding="basicHttpBinding" name="EpBasicHttp"
          contract="MyAirport.Entities.IAirportService" />
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
        <endpoint address="http://localhost:8734/MyAirport/AirportService/"
          binding="basicHttpBinding" bindingConfiguration="BindingBasicHttpUserCredential"
          name="EPBasicHttpUserCredential" contract="MyAirport.Entities.IAirportService" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/MyAirport/AirportService/" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="">
          <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
          <serviceDebug includeExceptionDetailInFaults="false" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>